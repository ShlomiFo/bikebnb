<h1> YOUR BOOKINGS</h1>

<div class="row">
    <% @bookings.each do |booking| %>
      <div class="co-12 col-sm-6 col-md-4">
        <%= link_to bookings_path(booking), class: "text-decoration-none" do %>
            <%= booking.start %>
            <%= booking.end %>
            <%= booking.bike.name %>
          </div>
        <% end %>
      </div>
    <% end %>
  </div>

  <%= link_to "Back to home", root_path %>
